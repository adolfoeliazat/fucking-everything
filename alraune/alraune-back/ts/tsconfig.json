{
  "compilerOptions": {
    "target": "es2015",
    "module": "none",
    "noImplicitAny": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "noFallthroughCasesInSwitch": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "outFile": "out/alraune.js",
    "sourceMap": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "**/*.spec.ts"
  ]
}
